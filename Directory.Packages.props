    <PackageVersion Include="OpenTelemetry.Instrumentation.Runtime" Version="0.1.0-alpha.1" />
    <PackageVersion Include="OpenTelemetry.Exporter.Console" Version="$(OpenTelemetryCoreVersion)" />

    <PackageVersion Include="Microsoft.Extensions.DependencyInjection" Version="$(MExVersion)" />
