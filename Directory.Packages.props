    <AspNetCoreVersion>6.0.6</AspNetCoreVersion>
    <MExVersion>6.0.0</MExVersion>
    <OpenTelemetryCoreVersion>1.3.0-rc.2</OpenTelemetryCoreVersion>
    <OpenTelemetryNonCoreVersion>1.0.0-rc9.3</OpenTelemetryNonCoreVersion>
    <DCAExtensionsVersion>0.0.1-alpha3.0</DCAExtensionsVersion>
