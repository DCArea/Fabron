<Project>

  <PropertyGroup>
    <DefaultNetCoreTargetFramework>net5.0</DefaultNetCoreTargetFramework>
  </PropertyGroup>

  <PropertyGroup>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <LangVersion>preview</LangVersion>
    <Nullable>enable</Nullable>
    <DebugType>portable</DebugType>
  </PropertyGroup>

  <!-- framework versions -->
  <PropertyGroup>
    <AspNetCoreVersion>5.0.0-rc.2.20455.2</AspNetCoreVersion>
    <MEXVersion>5.0.0-rc.2.*</MEXVersion>
    <CoreFXVersion>5.0.0-rc.2.20454.25</CoreFXVersion>
    <EFCoreVersion>5.0.0-rc.2.*</EFCoreVersion>
    <OrleansVersion>3.3.0</OrleansVersion>
  </PropertyGroup>

  <!-- library versions -->
  <PropertyGroup>
    <ApplicationInsightsVersion>2.15.0</ApplicationInsightsVersion>
    <SwashbuckleAspNetCoreVersion>5.5.1</SwashbuckleAspNetCoreVersion>
    <StyleCopAnalyzersVersion>1.0.0</StyleCopAnalyzersVersion>
  </PropertyGroup>

  <!-- test pkgs -->
  <PropertyGroup>
    <NBomberVersion>1.0.0</NBomberVersion>
    <MicrosoftTestSdkVersion>16.7.1</MicrosoftTestSdkVersion>
    <xUnitVersion>2.4.1</xUnitVersion>
    <xUnitRunnerVisualStudio>2.4.3</xUnitRunnerVisualStudio>
    <FluentAssertionsVersion>6.0.0-alpha0001</FluentAssertionsVersion>
    <CoverletCollectorVersion>1.3.0</CoverletCollectorVersion>
    <MartinCostelloLoggingXUnitVersion>0.1.0</MartinCostelloLoggingXUnitVersion>
    <OrleansTestKitVersion>3.1.2</OrleansTestKitVersion>
    <MoqVersion>4.14.5</MoqVersion>
  </PropertyGroup>
</Project>
