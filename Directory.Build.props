<Project>

  <PropertyGroup>
    <DefaultTargetFramework>net6.0</DefaultTargetFramework>
    <VersionPrefix>0.0.1</VersionPrefix>
  </PropertyGroup>

  <PropertyGroup>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <LangVersion>preview</LangVersion>
    <Nullable>enable</Nullable>
    <DebugType>portable</DebugType>
  </PropertyGroup>

  <!-- framework versions -->
  <PropertyGroup>
    <AspNetCoreVersion>6.0.4</AspNetCoreVersion>
    <MEXVersion>6.0.0</MEXVersion>
    <OrleansVersion>3.9.9-ci.20220518.1</OrleansVersion>
  </PropertyGroup>

  <!-- library versions -->
  <PropertyGroup>
    <AspNetCoreAuthenticationApiKeyVersion>5.1.0</AspNetCoreAuthenticationApiKeyVersion>
    <CassandraCSharpDriverVersion>3.17.0</CassandraCSharpDriverVersion>
    <CronosVersion>0.7.1</CronosVersion>
    <MapsterVersion>7.2.0</MapsterVersion>
    <MicrosoftToolkitDiagnostics>7.1.2</MicrosoftToolkitDiagnostics>
    <NpgsqlVersion>6.0.4</NpgsqlVersion>
    <OpenTelemetryCoreVersion>1.3.0-beta.1</OpenTelemetryCoreVersion>
    <OpenTelemetryNonCoreVersion>1.0.0-rc9.3</OpenTelemetryNonCoreVersion>
    <OrleansCassandraUtilsVersion>1.0.1</OrleansCassandraUtilsVersion>
    <OrleansClusteringRedisVersion>3.1.1</OrleansClusteringRedisVersion>
    <OrleansProvidersMongoDBVersion>3.4.1</OrleansProvidersMongoDBVersion>
    <PrometheusNetVersion>6.0.0</PrometheusNetVersion>
    <StyleCopAnalyzersVersion>1.0.0</StyleCopAnalyzersVersion>
    <SwashbuckleAspNetCoreVersion>6.3.1</SwashbuckleAspNetCoreVersion>
    <NESTVersion>7.15.2</NESTVersion>
  </PropertyGroup>

  <!-- test pkgs -->
  <PropertyGroup>
    <CoverletCollectorVersion>3.1.2</CoverletCollectorVersion>
    <FakeItEasyVersion>7.3.1</FakeItEasyVersion>
    <FluentAssertionsVersion>6.6.0</FluentAssertionsVersion>
    <MartinCostelloLoggingXUnitVersion>0.2.0</MartinCostelloLoggingXUnitVersion>
    <MicrosoftTestSdkVersion>17.1.0</MicrosoftTestSdkVersion>
    <MoqContribHttpClientVersion>1.3.0</MoqContribHttpClientVersion>
    <MoqVersion>4.17.2</MoqVersion>
    <OrleansTestKitVersion>3.1.7</OrleansTestKitVersion>
    <xUnitRunnerVisualStudio>2.4.3</xUnitRunnerVisualStudio>
    <xUnitVersion>2.4.1</xUnitVersion>
  </PropertyGroup>
</Project>
